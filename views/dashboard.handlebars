<section class="dashboard">
{{#each post as |post| }}
<div class='card' style='width: 18rem;'>
    <div class="card-body" id="dash">
        <h5 class="card-title">{{post.title}}{{post.user_id}} {{post.date_created}}</h5>
        <h6 class="card-subtitle mb-2 yexy-muted"></h6>
        <p class="card-text">{{post.content}}</p>
    </div>
</div>
{{/each}}
</section>